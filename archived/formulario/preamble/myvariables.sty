\newcommand\evidenzia[2]{
	\tikz[%
		baseline,
		remember picture]{
		\node[%
			draw = purple,
			fill = white,
			densely dashed,
			rectangle,
			rounded corners,
			inner sep = 3pt,
			anchor = base] (#1) {
			\(#2\)};
	}
}

